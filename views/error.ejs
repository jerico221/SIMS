<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("./components/header.ejs")%>
  </head>
  <body>
    <div id="layoutError">
      <div id="layoutError_content">
        <main>
          <div class="container">
            <div class="row justify-content-center">
              <div class="text-center mt-4">
                <img
                  class="mb-4 img-error"
                  src="images/img/error-404-monochrome.svg"
                  alt="Error 404"
                  style="width: 300px !important ; height: 300px !important"
                />
                <p class="lead">
                  This requested URL was not found on this server.
                </p>

                <h1><%= message %></h1>
                <h2><%= error.status %></h2>
                <pre><%= error.stack %></pre>
              </div>

              <a href="/" class="bg-danger">
                <i class="fas fa-arrow-left me-1"></i>
                Return to Login
              </a>
            </div>
          </div>
        </main>
      </div>
    </div>
  </body>
</html>
